# Archivo de configuración de Travabus
# Variables de entorno para el sistema de pagos

# Clave secreta de Stripe (comienza con sk_test_ o sk_live_)
STRIPE_SECRET_KEY=YOUR_STRIPE_SECRET_KEY

# Clave del webhook de Stripe (generada en el dashboard de Stripe)
STRIPE_WEBHOOK_SECRET=YOUR_STRIPE_WEBHOOK_SECRET

# URL de Supabase
SUPABASE_URL=YOUR_SUPABASE_URL

# Clave de servicio de Supabase
SUPABASE_SERVICE_ROLE_KEY=YOUR_SUPABASE_SERVICE_ROLE_KEY

# Puerto en el que correrá el servidor
PORT=3000